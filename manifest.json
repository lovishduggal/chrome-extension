{
    "name": "Color Picker",
    "description": "A simple color picker extension with clipboard functionality",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "32": "/img/dropper_24.png",
        "48": "/img/dropper_32.png",
        "64": "/img/dropper_64.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ]
}